{
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5",
    "phpspec/prophecy": "^1.16",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "BurritoFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BurritoFactory\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "akei/kulinarisk": "2.0.1",
    "cweagans/composer-patches": "^1.7"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./libs/Kulinarisk_V1.0",
      "canonical": false,
      "options": {
        "versions": {
          "akei/kulinarisk": "1.0"
        }
      }
    },
    {
      "type": "path",
      "url": "./libs/Kulinarisk_V2.0",
      "canonical": false,
      "options": {
        "versions": {
          "akei/kulinarisk": "2.0"
        }
      }
    },
    {
      "type": "path",
      "url": "./libs/Kulinarisk_V2.0.1",
      "canonical": false,
      "options": {
        "versions": {
          "akei/kulinarisk": "2.0.1"
        }
      }
    }
  ],
  "extra": {
    "patches": {
      "phpunit/phpunit": [
        "./patches/patch.diff"
      ]
    }
  }
}
